{
  "name": "social-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "generate:nexus": "export $(cat .env.local | xargs) && nodemon --exec \"ts-node -r tsconfig-paths/register --transpile-only -P nexus.tsconfig.json src/graphql/schema\" --ext \"ts\" --watch \"*/graphql/**/*.ts\"",
    "generate:genql": "nodemon --exec \"genql --schema ./src/graphql/generated/schema.graphql --output ./src/graphql/generated/genql\"  --watch \"src/graphql/generated/schema.graphql\"",
    "generate": "concurrently -k -n \"NEXUS,GENQL\" -c \"green,blue\" \"pnpm generate:nexus\" \"pnpm generate:genql\""
  },
  "dependencies": {
    "@genql/runtime": "^2.10.0",
    "@headlessui/react": "^1.6.4",
    "@heroicons/react": "^1.0.6",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "apollo-server-micro": "3.8.2",
    "argon2": "^0.28.5",
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "eslint-config-prettier": "^8.5.0",
    "graphql": "16.5.0",
    "graphql-request": "^4.3.0",
    "graphql-scalars": "^1.17.0",
    "graphql-tools": "^8.2.12",
    "isemail": "^3.2.0",
    "micro": "^9.3.4",
    "micro-cors": "^0.1.1",
    "moment": "^2.29.3",
    "mongodb": "^4.7.0",
    "mongoose": "^6.3.6",
    "next": "12.1.6",
    "next-auth": "^4.5.0",
    "nextjs-progressbar": "^0.0.14",
    "nexus": "^1.3.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-hook-form": "^7.32.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-infinite-scroll-hook": "^4.0.3",
    "react-moment": "^1.1.2",
    "react-query": "^3.39.1",
    "react-toastify": "^9.0.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@genql/cli": "^2.10.0",
    "@types/node": "^17.0.42",
    "@types/react": "^18.0.12",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.2.1",
    "dotenv": "^16.0.1",
    "eslint": "8.17.0",
    "eslint-config-next": "12.1.6",
    "nodemon": "^2.0.16",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "tailwindcss": "^3.1.2",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.3"
  }
}
